# 10k-code3
----------------------char类型元素个数大小--------------------------------
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>

int main()
{
	int arr[] = { 1,2,3,4,5,6,7,8,9 };
	char arr2[] = "123456789";
	int sz = sizeof(arr2) / sizeof(arr2[0]);
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		printf("arr[%d]=%p\n", i, &arr2[i]);
	}

	return 0;
}
arr[0]=000000E0A3DBFB48
arr[1]=000000E0A3DBFB49
arr[2]=000000E0A3DBFB4A
arr[3]=000000E0A3DBFB4B
arr[4]=000000E0A3DBFB4C
arr[5]=000000E0A3DBFB4D
arr[6]=000000E0A3DBFB4E
arr[7]=000000E0A3DBFB4F
arr[8]=000000E0A3DBFB50
arr[9]=000000E0A3DBFB51
---------------------int类型元素个数大小------------------------------------
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>

int main()
{
	int arr[] = { 1,2,3,4,5,6,7,8,9 };
	char arr2[] = "123456789";
	int sz = sizeof(arr) / sizeof(arr[0]);
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		printf("arr[%d]=%p\n", i, &arr[i]);
	}

	return 0;
}
arr[0]=0000002273AFFBD8
arr[1]=0000002273AFFBDC
arr[2]=0000002273AFFBE0
arr[3]=0000002273AFFBE4
arr[4]=0000002273AFFBE8
arr[5]=0000002273AFFBEC
arr[6]=0000002273AFFBF0
arr[7]=0000002273AFFBF4
arr[8]=0000002273AFFBF8
